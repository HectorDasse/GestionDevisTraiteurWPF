        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GestionDevisTraiteurWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="famille" Content="Famille" HorizontalAlignment="Left" Margin="451,121,0,0" VerticalAlignment="Top" Width="75" Click="ListeFamille"/>
        <Button x:Name="produit" Content="Produit" HorizontalAlignment="Left" Margin="258,121,0,0" VerticalAlignment="Top" Width="75" Click="ListeProduit"/>
        <Button x:Name="client" Content="Client" HorizontalAlignment="Left" Margin="258,179,0,0" VerticalAlignment="Top" Width="75" Click="ListeClient"/>
        <Button x:Name="scrapping" Content="scrapping" HorizontalAlignment="Left" Margin="451,179,0,0" VerticalAlignment="Top" Width="75" Click="Scrapping" Visibility="Hidden"/>
        <Button x:Name="Devis" Content="Devis" HorizontalAlignment="Left" Margin="451,179,0,0" VerticalAlignment="Top" Width="75" Click="ListeDevis"/>
    </Grid>
</Window>
